<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://schemas.xmlsoap.org/wsdl/" 
xmlns:tns="http://explanation.triplestore.smwplus.ontoprise.com/" 
xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" targetNamespace="http://explanation.triplestore.smwplus.ontoprise.com/" name="ExplanationEndpointService">
 
  <message name="explainQuery">
    <part name="arg0" type="xsd:string" />
    <part name="arg1" type="xsd:string" />

    
  </message>
  <message name="explainResponse">
    <part name="return" type="xsd:string" />
  </message>
 
 
  <portType name="ExplanationEndpoint">
    <operation name="explain" parameterOrder="arg0 arg1">

      <input message="tns:explainQuery" />
      <output message="tns:explainResponse" />
     
    </operation>
  

  </portType>
  <binding name="ExplanationEndpointPortBinding"
    type="tns:ExplanationEndpoint">

    <soap:binding style="rpc"
        transport="http://schemas.xmlsoap.org/soap/http" />
    <operation name="explain">

        <soap:operation
            soapAction="http://explanation.triplestore.smwplus.ontoprise.com/explain" />
        <input>

            <soap:body use="literal"
                namespace="http://explanation.triplestore.smwplus.ontoprise.com/" />
        </input>
        <output>

            <soap:body use="literal"
                namespace="http://explanation.triplestore.smwplus.ontoprise.com/" />
        </output>
        
    </operation>

   

  </binding>
  <service name="ExplanationEndpointService">
    <port name="ExplanationEndpointPort" binding="tns:ExplanationEndpointPortBinding">
      <soap:address location="http://{{webservice-endpoint}}/explain" />
    </port>
  </service>
</definitions>